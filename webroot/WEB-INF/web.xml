<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" 
	xmlns="http://java.sun.com/xml/ns/javaee" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
	http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">	
  <servlet>
    <servlet-name>UploadServlet</servlet-name>
    <servlet-class>servlet.UploadServlet</servlet-class>
</servlet>
  <servlet>
    <servlet-name>Login</servlet-name>
    <servlet-class>servlet.Login</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>Thesis_upload</servlet-name>
    <servlet-class>servlet.Thesis_upload</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>Msgsender</servlet-name>
    <servlet-class>servlet.Msgsender</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>ProfileUpdate</servlet-name>
    <servlet-class>servlet.ProfileUpdate</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>ProposalUpdate</servlet-name>
    <servlet-class>servlet.ProposalUpdate</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>MessageGet</servlet-name>
    <servlet-class>servlet.MessageGet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>MessageSent</servlet-name>
    <servlet-class>servlet.MessageSent</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>AddStudent</servlet-name>
    <servlet-class>admin.AddStudent</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>AddTeacher</servlet-name>
    <servlet-class>admin.AddTeacher</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>AddAdmin</servlet-name>
    <servlet-class>admin.AddAdmin</servlet-class>
  </servlet>











 
<servlet-mapping>
    <servlet-name>UploadServlet</servlet-name>
    <url-pattern>/upload</url-pattern>
</servlet-mapping>
  <servlet-mapping>
    <servlet-name>Login</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Thesis_upload</servlet-name>
    <url-pattern>/thesis-upload</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Msgsender</servlet-name>
    <url-pattern>/message-sender</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ProfileUpdate</servlet-name>
    <url-pattern>/profile-update</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ProposalUpdate</servlet-name>
    <url-pattern>/proposal-update</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>MessageGet</servlet-name>
    <url-pattern>/message-get</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>MessageSent</servlet-name>
    <url-pattern>/message-sent</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>AddStudent</servlet-name>
    <url-pattern>/admin/addstudent</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>AddTeacher</servlet-name>
    <url-pattern>/admin/addteacher</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>AddAdmin</servlet-name>
    <url-pattern>/admin/addadmin</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
</web-app>
